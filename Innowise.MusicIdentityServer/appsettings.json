{
  "ConnectionStrings": {
    "PostgresAppDbConnection": "Server=localhost;Port=5432;Database=musicidentity;User Id=admin;Password=admin"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "JwtSettings": {
    "Issuer": "Innowise.MusicIdentityServer",
    "Audience": "Innowise.Music",
    "Duration": 1,
    "Key": "075ee246-d7d6-4089-84ea-1246f95b9014"
  },
  "AllowedHosts": "*"
}
